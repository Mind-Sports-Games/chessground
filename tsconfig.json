{
  "include": ["src/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "dist",
    "noEmitOnError": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "es2017",
    "lib": ["DOM", "es2017"]
  }
}
